  "scripts": {
    "ng": "node  --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "start:dev": "ng serve --configuration development --host 0.0.0.0",
    "start:dev-4201": "ng start:dev --port 4201",
    "start:prod": "ng serve --configuration production --host 0.0.0.0",
    "start:hmr": "ng serve --configuration hmr --host 0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --aot --configuration production",
    "build:analyze": "ng build --configuration production --stats-json && ng analyze",
    "analyze": "webpack-bundle-analyzer ./dist/analyze-result.json",
    "clean": "rimraf dist node_modules",
    "cypr": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9299 cypress open",
    "lint": "ng lint"
  },
